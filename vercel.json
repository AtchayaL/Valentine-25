{
  "version": 2,
  "builds": [
    {
      "src": "Val_Card/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "main_folder/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "main_folder/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "main_folder/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.mp3",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/main_folder/(.*)",
      "dest": "/main_folder/$1"
    },
    {
      "src": "/Val_Card/(.*)",
      "dest": "/Val_Card/$1"
    },
    {
      "src": "/",
      "dest": "/Val_Card/index.html"  
    }
  ]
}
