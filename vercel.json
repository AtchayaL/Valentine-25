{
  "version": 2,
  "builds": [
    {
      "src": "Val_Card/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "Val_Card/*.mp3",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Val_Card/index.html"  // All requests go to index.html
    }
  ]
}
